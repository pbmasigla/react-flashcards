{
	"presets": [ "es2015-loose", "react", "stage-0" ],
	"plugins": [
		["transform-decorators-legacy"],
		["react-transform", {
			"transforms": [{
				"transform": "react-transform-hmr",
				"imports": ["react"],
				"locals": ["module"]
			}, {
				"transform": "react-transform-catch-errors",
				"imports": ["react", "redbox-react"]
			}]
		}]
	]
}
